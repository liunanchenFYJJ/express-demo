{
  "name": "express-demo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "set NODE_ENV=dev&&yarn run build&&node meadowlark",
    "prod_win": "set NODE_ENV=prod&&node meadowlark",
    "prod_mac": "NODE_ENV=prod node meadowlark",
    "pm2": "pm2 start pm2.json --env env",
    "pm2_prod": "pm2 start pm2.json --env prod",
    "test": "jest",
    "build_sass": "node-sass public/css -o dist/css",
    "watch_sass": "node-sass -w -r public/css -o dist/css"
  },
  "dependencies": {
    "@types/node": "^11.10.4",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express3-handlebars": "^0.5.2",
    "http-errors": "~1.6.2",
    "i18next": "^15.0.2",
    "i18next-express-middleware": "^1.7.1",
    "i18next-node-fs-backend": "^2.1.1",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "mysql": "^2.16.0",
    "socket.io": "^2.1.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "gulp": "^4.0.0",
    "gulp-nodemon": "^2.4.2",
    "gulp-sass": "^4.0.2",
    "jest": "^23.6.0",
    "node-sass": "^4.11.0",
    "typescript": "^3.3.3333"
  }
}
